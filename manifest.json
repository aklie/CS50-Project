{
  "manifest_version": 2,

  "name": "Web Draw",
  "description": "This extension allows user to draw on webpages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Start Drawing!"
  },
  "icons": {
          "128": "listicon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
